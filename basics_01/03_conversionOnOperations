

let score = undefined

console.log(typeof (score));

let valueInNumber = Number(score);

console.log(typeof valueInNumber);

console.log(valueInNumber);

// 33abcor undefined output NaN  if its gets score=null it output will be 0
// "33"=> 33
// "33abc'=> NaN
// true => 1; false => 0

let isLoggedIn = 0 // 0 false 1 true

let booleanIsLoggedIn = Boolean(isLoggedIn); //conversion 
console.log(booleanIsLoggedIn);

// true => 1; false => 0
// "" => false
// "Prathmesh"=> true


let someNumber = 33;
